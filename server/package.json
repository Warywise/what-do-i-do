{
  "name": "what-do-i-do-server",
  "version": "1.0.0",
  "description": "Hosts an API that will serve data to a to-do list",
  "main": "src/index.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "npm run build && node ./dist/src/index.js",
    "build": "tsc",
    "dev": "nodemon --watch src src/index.ts js,ts",
